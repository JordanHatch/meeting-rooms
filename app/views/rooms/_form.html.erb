<%= semantic_form_for room do |f| %>
  <%= f.inputs do %>
    <%= f.input :title %>
    <%= f.input :short_title %>
    <%= f.input :calendar_id, label: "Google Calendar ID" %>
  <% end %>

  <%= f.inputs "Custom messages" do %>
    <%= f.input :custom_free_message %>
    <%= f.input :custom_busy_message %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :button %>
    <%= f.action :cancel, :as => :link %>
  <% end %>
<% end %>
