<%= semantic_form_for room do |f| %>
  <%= f.inputs :title, :short_title, :calendar_id %>

  <%= f.inputs "Custom messages" do %>
    <%= f.input :custom_free_message %>
    <%= f.input :custom_busy_message %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :button %>
    <%= f.action :cancel, :as => :link %>
  <% end %>
<% end %>
