<h1><%= room.title %></h1>

<section class="events">
  <h1>Coming up</h1>

  <ol class="event-list">
    <%= render partial: 'event', collection: presented_room.events_with_gaps %>
  </ol>
</section>

<ul class="actions">
  <li><%= link_to 'Edit room', edit_room_path(room) %></li>
  <li><%= link_to 'Import events', import_room_path(room), method: :post %></li>
</ul>
